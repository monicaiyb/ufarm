        button.navbar-toggler(type="button" data-toggle="collapse" data-target="#basicExampleNav" aria-controls="basicExampleNav" aria-expanded="false" aria-label="Toggle navigation")
          span.navbar-toggler-icon
        // Collapsible content
        #basicExampleNav.collapse.navbar-collapse
          // Links
          ul.navbar-nav.mr-auto
            li.nav-item.active
              a.nav-link(href="#home")
                | Home
                span.sr-only (current)
            li.nav-item
              a.nav-link(href="#carousel") Products
            li.nav-item
              a.nav-link(href="#farmer") Farmers
            li.nav-item
              a.nav-link(href="#contacts") Contacts
      block content