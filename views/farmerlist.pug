extends layout
block content
    .admin-panel.clearfix
      .slidebar
        ul
          li
            a(href="") Add Farmer
          li
            a(href="#") Inspection
          li
            a(href="#Inspect") Approve Farmers                
          li
            a(href="#Approve") Add Products
          li
            a(href="#Repor") Report
          li
            a(href="#Logout") Logout
        
      .dash_display
        header.card-header
          h4.card-title Welcome 
        table.table
          if fos.length
                thead
                    tr
                    th Firstname
                    th Lastname
                    th gender
                    th ward
                    th 
                    th
                tbody
                  each fo in fos
                    tr
                        td #{fo.fname}
                        td=fo.lname
                        td=fo.gender
                        td=fo.ward
                        td  
                            a(href='/fo/Update/'+fos._id) Update
                        td